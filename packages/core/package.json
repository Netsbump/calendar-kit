{
  "name": "@calendar/core",
  "version": "0.1.0",
  "description": "Core calendar logic for calendar-kit",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --config tsup.config.ts",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "biome check .",
    "format": "biome format --write ."
  },
  "keywords": [
    "calendar",
    "date",
    "scheduler",
    "headless"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.2.2",
    "vitest": "^1.0.0",
    "@vitest/coverage-v8": "^1.0.0"
  }
} 